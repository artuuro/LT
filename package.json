{
  "name": "starter",
  "version": "0.0.8",
  "description": "API Boilerplate using Fastify & MongoDB",
  "type": "module",
  "main": "index.mjs",
  "scripts": {
    "dev": "set NODE_ENV=development && nodemon --es-module-specifier-resolution=node --experimental-modules --no-warnings --inspect ./server",
    "prod": "set NODE_ENV=production && node --es-module-specifier-resolution=node --experimental-modules --no-warnings ./server"
  },
  "author": "Arthur G.",
  "license": "UNLICENSED",
  "dependencies": {
    "fastify": "^2.3.0",
    "fastify-graceful-shutdown": "^2.0.1",
    "fastify-helmet": "^3.0.0",
    "fastify-mongoose": "^0.2.1",
    "fastify-static": "^2.4.0",
    "fastify-swagger": "^2.3.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.3",
    "web-push": "^3.3.5"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "certification/*"
    ]
  },
  "devDependencies": {}
}
